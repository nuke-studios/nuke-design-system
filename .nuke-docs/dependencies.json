{
  "_purpose": "Libraries and packages used",
  "_schema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
      "_purpose": {
        "description": "Purpose of this file",
        "type": "string"
      },
      "items": {
        "items": {
          "properties": {
            "description": {
              "type": "string"
            },
            "id": {
              "pattern": "^dep-[0-9]+$",
              "type": "string"
            },
            "title": {
              "type": "string"
            }
          },
          "required": [
            "id",
            "title"
          ],
          "type": "object"
        },
        "type": "array"
      }
    },
    "required": [
      "_purpose",
      "items"
    ],
    "type": "object"
  },
  "items": []
}